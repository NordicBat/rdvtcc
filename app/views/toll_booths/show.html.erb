<%- model_class = TollBooth -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:date) %>:</strong></dt>
  <dd><%= @toll_booth.date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:total) %>:</strong></dt>
  <dd><%= @toll_booth.total %></dd>
  <dt><strong><%= model_class.human_attribute_name(:travel_id) %>:</strong></dt>
  <dd><%= @toll_booth.travel_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:image) %>:</strong><%= image_tag @toll_both.image.thumb.url %></dt>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              travel_path(@travel), :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
               edit_travel_toll_booth_path(@parent, @toll_booth), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              [@travel, @toll_booth],
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
