<%- model_class = Travel -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:city_id) %></th>
      <th><%= model_class.human_attribute_name(:initialDate) %></th>
      <th><%= model_class.human_attribute_name(:finalDate) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th><%= model_class.human_attribute_name(:vehicle_id) %></th>
      <th><%= model_class.human_attribute_name(:meal_id) %></th>
      <th><%= model_class.human_attribute_name(:fuel_id) %></th>
      <th><%= model_class.human_attribute_name(:lodging_id) %></th>
      <th><%= model_class.human_attribute_name(:toll_both_id) %></th>
      <th><%= model_class.human_attribute_name(:finalOdometer) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @travels.each do |travel| %>
      <tr>
        <td><%= link_to travel.id, travel_path(travel) %></td>
        <td><%= travel.city_id %></td>
        <td><%= travel.initialDate %></td>
        <td><%= travel.finalDate %></td>
        <td><%= travel.description %></td>
        <td><%= travel.vehicle_id %></td>
        <td><%= travel.meal_id %></td>
        <td><%= travel.fuel_id %></td>
        <td><%= travel.lodging_id %></td>
        <td><%= travel.toll_both_id %></td>
        <td><%= travel.finalOdometer %></td>
        <td><%=l travel.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_travel_path(travel), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      travel_path(travel),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_travel_path,
            :class => 'btn btn-primary' %>
